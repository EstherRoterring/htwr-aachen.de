    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "autoprefixer": "^10.4.14",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "cssnano": "^5.1.15",
    "cypress": "^12.8.1",
    "eslint": "^8.36.0",
    "eslint-config-airbnb-base": "^15.0.0",
