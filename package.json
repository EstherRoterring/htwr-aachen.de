    "next-mdx-remote": "^4.4.1",
    "next-remote-watch": "^2.0.0",
    "next-seo": "^5.15.0",
    "next-sitemap": "^3.1.55",
    "nextjs-breadcrumbs": "^1.1.9",
    "parse5": "^7.1.2",
    "prismjs": "^1.29.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
